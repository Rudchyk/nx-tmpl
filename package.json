{
  "name": "nx-tmpl",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "all": "nx run-many --target=serve --projects=api,client --parallel=true",
    "build": "nx build",
    "test": "nx test",
    "pregenzip": "cd ./tools/generators && make clean",
    "genzip": "cd ./tools/generators && make zip",
    "p": "nx workspace-generator element --directory=\"pages\" --no-interactive --name",
    "m": "nx workspace-generator element --directory=\"modules\" --no-interactive --name",
    "t": "nx workspace-generator element --directory=\"templates\" --no-interactive --name",
    "l": "nx workspace-generator element --directory=\"layouts\" --no-interactive --name",
    "c": "nx workspace-generator element --directory=\"components\" --no-interactive --name",
    "f": "nx workspace-generator form --no-interactive --name",
    "ui:c": "nx workspace-generator lib-element --directory=\"components\" --no-interactive --name",
    "service": "nx workspace-generator service --no-interactive --name",
    "store": "nx workspace-generator redux --no-interactive --name",
    "hook": "nx workspace-generator hook --no-interactive --name",
    "hook:l": "nx workspace-generator hook --lib=\"hooks\" --no-interactive --name",
    "util": "nx workspace-generator util --no-interactive --name",
    "util:c": "nx workspace-generator util --lib=\"client\" --no-interactive --name",
    "util:api": "nx workspace-generator util --lib=\"api\" --no-interactive --name",
    "const": "nx workspace-generator constant --no-interactive --name",
    "enum": "nx workspace-generator constant --no-interactive --type=\"enum\" --name",
    "int": "nx workspace-generator interface --no-interactive --name",
    "api:c": "nx workspace-generator express-controller --no-interactive --name",
    "api:v": "nx workspace-generator express-validator --no-interactive --name",
    "api:d": "nx workspace-generator express-decorator --no-interactive --name",
    "model": "nx workspace-generator mongoose-model --no-interactive --name",
    "route": "nx workspace-generator express-route --no-interactive --name",
    "api:s": "nx workspace-generator api-service --no-interactive --name"
  },
  "private": true,
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.18.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/cli": "14.5.1",
    "@nrwl/cypress": "14.5.1",
    "@nrwl/eslint-plugin-nx": "14.5.1",
    "@nrwl/express": "14.5.1",
    "@nrwl/jest": "14.5.1",
    "@nrwl/linter": "14.5.1",
    "@nrwl/node": "14.5.1",
    "@nrwl/react": "14.5.1",
    "@nrwl/web": "14.5.1",
    "@nrwl/workspace": "14.5.1",
    "@testing-library/react": "13.3.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "nx": "14.5.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  }
}
