{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "element",
  "title": "Nx Element Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Element name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "type": {
      "type": "string",
      "enum": ["constant", "enum"],
      "default": "constant",
      "description": "The type of the new constant."
    }
  },
  "required": ["name"]
}
